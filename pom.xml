	<groupId>de.sfb876</groupId>
	<artifactId>streams-cta</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<dependencies>
		<dependency>
			<groupId>org.jwall</groupId>
			<artifactId>streams-runtime</artifactId>
			<version>[0.9.20,)</version>
